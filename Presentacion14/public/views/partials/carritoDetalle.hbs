{{#each producto}}
<tr>
    <td><img style="display: inline-flex; width: 50px; height: 50px" src="{{fotoProducto}}" class="imgTable"/></td>
    <td>{{idProducto}}</td>
    <td><a onclick="detail({{idProducto}})">{{nombreProducto}} </a> </td>   
    <td>${{precioProducto}}</td>   
    <td>{{quantity}}</td>     
    <td>${{subtotal}}</td>    
    <td><button onclick="deleteItemCart({{idProducto}},{{idCarrito}})">Borrar</button></td>
</tr>

{{/each}}   

{{> footer }}